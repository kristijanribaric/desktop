{
  "remove": {
    "identifiers": [
      // Firefox adds Perplexity by default, we don't want it as
      // its not very privacy focused.
      "perplexity",

      // These are not search engines, Firefox adds them by default
      // but we don't want them.
      "wikipedia",
      "wikipedia-*",
      "ebay",
      "ebay-*"
    ]
  }
}
