{
  "bracketSameLine": true,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "jsxSingleQuote": false,
  "semi": true,
  "printWidth": 100,
  "plugins": ["prettier-plugin-sh"],
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "parser": "css",
        "printWidth": 160
      }
    }
  ]
}
